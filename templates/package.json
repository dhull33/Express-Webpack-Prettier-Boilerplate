{
  "name": "YOUR PROJECT NAME",
  "description": "",
  "version": "1.0.0",
  "author": "",
  "license": "",
  "repository": {
    "type": "git",
    "url": "URL TO YOUR REPOSITORY"
  },
  "bugs": {
    "url": "URL TO ISSUE TRACKER"
  },
  "scripts": {
    "clean": "rimraf DIRECTORY TO CLEAN OUT",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start:prettier:dev": "npm-run-all --parallel open:src:dev prettier-watch",
    "clean:build:dev": "run-s clean build open:src:dev",
    "open:src:dev": "nodemon bin/www",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "build": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "~3.1.3",
    "express": "~4.17.1",
    "express-sslify": "^1.2.0",
    "http-errors": "~1.7.3",
    "morgan": "~1.10.0",
    "pm2": "^4.4.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "ajv": "^6.12.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-airbnb": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "babel-plugin-transform-class-properties"
    ]
  }
}
